{
  "name": "clean-context-menu",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Cleans up the context menus when you find they are getting too large.",
  "keywords": [
  ],
  "repository": "https://github.com/m-hall/clean-context-menu",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
  },
  "eslintConfig": {
    "rules": {
      "no-empty": 0,
      "no-console": 0,
      "no-new": 0,
      "no-extra-semi": 1,
      "semi": [
        2,
        "always"
      ],
      "func-names": 0,
      "strict": [
        0,
        "global"
      ],
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ],
      "comma-dangle": 0
    },
    "globals": {
      "atom": "true"
    },
    "env": {
      "es6": true,
      "node": true
    }
  }
}
